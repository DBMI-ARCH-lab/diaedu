<div class="diaedu-content">
  <div class="filters">
    {{render "kb_filter_set" filterSet}}
  </div>
  <div class="main">
    <h1>
      <img {{bindAttr src="iconPath"}} />
      {{data_type.title}}
    </h1>

    {{#if loading}}
      <div class="spinner">{{i18n loading}}</div>
    {{else}}
      
      {{#if noObjs}}
        {{i18n kb.common.none_found}}
      {{else}}
        {{partial 'diaedu/templates/kb_objs/_index_list'}}
        {{partial 'diaedu/templates/kb_objs/_pagination'}}
      {{/if}}
    {{/if}}
  </div>
</div>